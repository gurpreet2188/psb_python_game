<?xml version="1.0" encoding="utf-8"?>
<resources>
    <attr name="backgroundColor" format="integer" />
    <attr name="fillColor" format="integer" />
    <attr name="singleToneColor" format="integer" />



    <style name="DualToneDarkTheme">
        <item name="backgroundColor">@*com.android.systemui:color/dark_mode_icon_color_dual_tone_background</item>
        <item name="fillColor">@*com.android.systemui:color/dark_mode_icon_color_dual_tone_fill</item>
        <item name="singleToneColor">@*com.android.systemui:color/dark_mode_icon_color_single_tone</item>
    </style>
    <style name="DualToneLightTheme">
        <item name="backgroundColor">@*com.android.systemui:color/light_mode_icon_color_dual_tone_background</item>
        <item name="fillColor">@*com.android.systemui:color/light_mode_icon_color_dual_tone_fill</item>
        <item name="singleToneColor">@*com.android.systemui:color/light_mode_icon_color_single_tone</item>
    </style>

</resources>