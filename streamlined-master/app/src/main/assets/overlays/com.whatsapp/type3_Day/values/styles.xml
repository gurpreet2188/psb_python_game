<resources>

    <style name="Toolbar" parent="@*com.whatsapp:style/ThemeOverlay.AppCompat.Dark.ActionBar">

        <item name="android:elevation">0.0dip</item>
        <item name="android:textColorPrimary">@*com.whatsapp:color/primary_text_default_material_light</item>
        <item name="android:textAppearanceMedium">@*com.whatsapp:style/TextAppearanceMenuList</item>
        <item name="*com.whatsapp:colorControlNormal">#d81e1e1e</item>
        <item name="*com.whatsapp:colorControlActivated">#1e1e1e</item>
        <item name="*com.whatsapp:colorBackgroundFloating">@*com.whatsapp:color/background_floating_material_light</item>
    </style>

    <style name="WhatsAppButton">
        <item name="android:textSize">14.0sp</item>
        <item name="android:textColor">#1e1e1e</item>
        <item name="android:ellipsize">end</item>
        <item name="android:gravity">center</item>
        <item name="android:paddingLeft">24.0dip</item>
        <item name="android:paddingTop">0.0dip</item>
        <item name="android:paddingRight">24.0dip</item>
        <item name="android:paddingBottom">0.0dip</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">36.0dip</item>
        <item name="android:minHeight">36.0dip</item>
        <item name="android:singleLine">true</item>
        <item name="android:textAllCaps">true</item>
    </style>

