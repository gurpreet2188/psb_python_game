    <style name="Theme.App.GalleryPicker" parent="@*com.whatsapp:style/Theme.App">
        <item name="android:colorControlNormal">#d81e1e1e</item>
        <item name="android:titleTextColor">#1e1e1e</item>
        <item name="android:windowLightStatusBar">true</item>
        <item name="android:windowBackground">@android:color/background_light</item>
        <item name="*com.whatsapp:actionOverflowButtonStyle">@*com.whatsapp:style/OverFlowLight</item>
        <item name="*com.whatsapp:actionBarStyle">@*com.whatsapp:style/Widget.Styled.ActionBar.GalleryPicker</item>
        <item name="*com.whatsapp:actionModeBackground">@android:color/holo_red_light</item>
        <item name="*com.whatsapp:homeAsUpIndicator">@*com.whatsapp:drawable/ic_back_teal</item>
        <item name="*com.whatsapp:searchViewStyle">@*com.whatsapp:style/GalleryPickerSearchView</item>
        <item name="*com.whatsapp:colorPrimary">@android:color/holo_red_light</item>
        <item name="*com.whatsapp:colorPrimaryDark">@android:color/holo_red_light</item>
        <item name="*com.whatsapp:colorAccent">@*android:color/accent_material_light</item>
    </style>

    <style name="AttachPickerLabel">
        <item name="android:textColor">#c11e1e1e</item>
        <item name="android:ellipsize">end</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:maxLines">1</item>
        <item name="android:singleLine">true</item>
    </style>

    <style name="Theme.App.Home" parent="@*com.whatsapp:style/Theme.App">
        <item name="android:colorActivatedHighlight">#1e1e1e</item>
        <item name="android:colorControlActivated">#1e1e1e</item>
        <item name="android:textColorPrimaryDisableOnly">#1e1e1e</item>
        <item name="android:textColorPrimary">#1e1e1e</item>
        <item name="android:textColorSecondary">#d81e1e1e</item>
        <item name="android:colorControlNormal">#d81e1e1e</item>
        <item name="android:navigationBarColor">@android:color/background_light</item>

        <item name="android:windowBackground">@android:color/background_light</item>
        <item name="android:fastScrollThumbDrawable">@*com.whatsapp:drawable/fastscroll_thumb_home</item>
        <item name="android:fastScrollTrackDrawable">@*com.whatsapp:drawable/fastscroll_track</item>
        <item name="*com.whatsapp:windowActionBar">false</item>
        <item name="*com.whatsapp:windowNoTitle">true</item>
    </style>
