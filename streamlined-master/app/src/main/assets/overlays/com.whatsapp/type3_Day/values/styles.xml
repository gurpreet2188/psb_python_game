    <style name="CardSeparator" parent="@android:style/Widget">
        <item name="android:layout_gravity">bottom</item>
        <item name="android:background">@android:color/background_light</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">0.5dip</item>
    </style>

    <style name="ActionBarButtonStyle" parent="@*com.whatsapp:style/Widget.AppCompat.ActionButton">
        <item name="android:windowBackground">@*android:color/background_material_light</item>
    </style>

    <style name="Text" parent="@android:style/TextAppearance">
        <item name="android:textColor">#d81e1e1e</item>
        <item name="android:shadowDx">1.0</item>
        <item name="android:shadowDy">1.0</item>
        <item name="android:shadowRadius">0.0</item>
        <item name="android:lineSpacingMultiplier">1.25</item>
    </style>

    <style name="FlatCardSeparator" parent="@android:style/Widget">
        <item name="android:background">@android:color/background_light</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">0.5dip</item>
        <item name="android:layout_marginLeft">@*com.whatsapp:dimen/info_screen_padding</item>
    </style>

    <style name="Base.Widget.AppCompat.ActionButton" parent="@android:style/Widget.Material.ActionButton" >
        <item name="android:color">#1e1e1e</item>
        <item name="android:tint">#1e1e1e</item>
    </style>


</resources>

