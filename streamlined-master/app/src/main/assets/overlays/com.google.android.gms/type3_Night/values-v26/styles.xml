        <item name="android:statusBarColor">@android:color/background_dark</item>
        <item name="android:navigationBarColor">@android:color/background_dark</item>
        <item name="android:windowLightStatusBar">false</item>
        <item name="android:windowLightNavigationBar">false</item>
        <item name="*com.google.android.gms:colorAccent">@android:color/holo_blue_bright</item>
        <item name="*com.google.android.gms:colorPrimary">@android:color/background_dark</item>
        <item name="*com.google.android.gms:colorPrimaryDark">@android:color/background_dark</item>
        <item name="*com.google.android.gms:preferenceTheme">@*com.google.android.gms:style/PreferenceTheme</item>
