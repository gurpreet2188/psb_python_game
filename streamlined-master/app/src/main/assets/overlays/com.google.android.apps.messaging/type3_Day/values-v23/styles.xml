<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="ColorAccentBlueOverrideStyle">
        <item name="*com.google.android.apps.messaging:colorAccent">@*android:color/accent_material_light</item>
    </style>


    <style name="BugleBaseTheme.ActionBar" parent="@*com.google.android.apps.messaging:style/BugleBaseTheme">
        <item name="android:windowDrawsSystemBarBackgrounds">true</item>
        <item name="android:windowLightStatusBar">true</item>
        <item name="*com.google.android.apps.messaging:actionBarBackgroundColorM2">@android:color/background_light</item>
    </style>
    <style name="BugleBaseTheme.NoActionBar" parent="@*com.google.android.apps.messaging:style/BugleBaseTheme">
        <item name="android:windowDrawsSystemBarBackgrounds">true</item>
        <item name="android:windowLightStatusBar">true</item>
        <item name="*com.google.android.apps.messaging:actionBarBackgroundColorM2">@android:color/background_light</item>
    </style>
</resources>