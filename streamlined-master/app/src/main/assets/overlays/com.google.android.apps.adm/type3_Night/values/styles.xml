<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="AppBaseTheme" parent="@*com.google.android.apps.adm:style/Theme.AppCompat.Light.DarkActionBar">
        <item name="*com.google.android.apps.adm:windowActionBarOverlay">true</item>
        <item name="*com.google.android.apps.adm:actionOverflowButtonStyle">@*com.google.android.apps.adm:style/OverflowButton</item>
        <item name="*com.google.android.apps.adm:actionBarStyle">@*com.google.android.apps.adm:style/ActionBar</item>
        <item name="*com.google.android.apps.adm:actionMenuTextColor">#ffffff</item>
        <item name="*com.google.android.apps.adm:popupMenuStyle">@*com.google.android.apps.adm:style/Widget.FindMyPhone.PopupMenu</item>
        <item name="*com.google.android.apps.adm:colorPrimary">@android:color/holo_blue_light</item>
        <item name="*com.google.android.apps.adm:colorPrimaryDark">@android:color/holo_blue_light</item>
        <item name="*com.google.android.apps.adm:colorAccent">@android:color/holo_blue_bright</item>
        <item name="*com.google.android.apps.adm:alertDialogTheme">@*com.google.android.apps.adm:style/AlertDialogTheme</item>
    </style>

    <style name="AlertDialogTheme" parent="@*com.google.android.apps.adm:style/Theme.AppCompat.Dialog.Alert">
        <item name="*com.google.android.apps.adm:colorPrimary">@android:color/holo_blue_light</item>
        <item name="*com.google.android.apps.adm:colorPrimaryDark">@android:color/holo_blue_light</item>
        <item name="*com.google.android.apps.adm:colorAccent">@android:color/holo_blue_bright</item>
    </style>

    <style name="DialogTheme" parent="@*com.google.android.apps.adm:style/Theme.AppCompat.Dialog">
        <item name="*com.google.android.apps.adm:colorPrimary">@android:color/holo_blue_light</item>
        <item name="*com.google.android.apps.adm:colorPrimaryDark">@android:color/holo_blue_light</item>
        <item name="*com.google.android.apps.adm:colorAccent">@android:color/holo_blue_bright</item>
    </style>

    <style name="Base.TextAppearance.AppCompat.Subhead" parent="@*com.google.android.apps.adm:style/Base.TextAppearance.AppCompat">
        <item name="android:textSize">@*com.google.android.apps.adm:dimen/abc_text_size_subhead_material</item>
        <item name="android:textColor">#f1ffffff</item>
    </style>
</resources>