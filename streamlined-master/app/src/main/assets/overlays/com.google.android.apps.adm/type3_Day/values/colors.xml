    <color name="primary_dark_material_light">@android:color/holo_red_light</color>
    <color name="primary_material_dark">@android:color/holo_red_light</color>
    <color name="primary_material_light">@android:color/holo_red_light</color>
    <color name="status_bar_color_grey">@android:color/holo_red_light</color>
