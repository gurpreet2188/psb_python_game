        <item name="*com.google.android.apps.adm:colorPrimary">@android:color/holo_red_light</item>
        <item name="*com.google.android.apps.adm:colorPrimaryDark">@android:color/holo_red_light</item>
        <item name="*com.google.android.apps.adm:colorAccent">@*android:color/accent_material_light</item>
        <item name="*com.google.android.apps.adm:alertDialogTheme">@*com.google.android.apps.adm:style/AlertDialogTheme</item>
