<?xml version="1.0" encoding="utf-8"?>
<resources>
   
    <style name="Platform.AppCompat" parent="@android:style/Theme.Material.Light">
        <item name="android:colorControlNormal">#1e1e1e</item>
        <item name="android:titleTextColor">#1e1e1e</item>
        <item name="android:windowLightStatusBar">true</item>
        <item name="android:windowBackground">@android:color/background_light</item>
        <item name="android:navigationBarColor">@android:color/background_light</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:textColorLink">?android:colorAccent</item>
        <item name="android:windowActionBar">false</item>
        <item name="android:buttonBarStyle">?buttonBarStyle</item>
        <item name="android:buttonBarButtonStyle">?buttonBarButtonStyle</item>
        <item name="android:textColorLinkInverse">?android:colorAccent</item>
    </style>

    <style name="DialogStyle" parent="@*android:Theme.Material.Light.Dialog">
        <item name="android:colorAccent">@android:color/holo_blue_bright</item>
        <item name="android:windowBackground">@android:color/background_light</item>
        <item name="android:colorPrimary">@android:color/holo_red_light</item>
        <item name="android:colorPrimaryDark">@android:color/holo_red_light</item>
    </style>


    <style name="PreferenceTheme" parent="@*com.xda.labs:style/Theme.AppCompat.Light">
        <item name="android:colorPrimary">@android:color/holo_red_light</item>
        <item name="android:colorPrimaryDark">@android:color/holo_red_light</item>
        <item name="android:colorAccent">@android:color/holo_blue_bright</item>
        <item name="android:windowActionBar">true</item>
        <item name="android:windowNoTitle">false</item>
    </style>

    <style name="Widget.AppCompat.DrawerArrowToggle" parent="@*com.xda.labs:style/Base.Widget.AppCompat.DrawerArrowToggle">
