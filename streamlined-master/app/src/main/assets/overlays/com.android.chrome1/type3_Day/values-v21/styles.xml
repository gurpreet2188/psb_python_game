<?xml version="1.0" encoding="utf-8"?>
<resources>
   
    <style name="ThemeWithActionBarBase" parent="@*com.android.chrome:style/Theme.AppCompat.Light.DarkActionBar">

        <item name="android:windowBackground">@android:color/background_light</item>
        <item name="*com.android.chrome:colorPrimary">@android:color/holo_red_light</item>
        <item name="*com.android.chrome:colorPrimaryDark">@android:color/holo_red_light</item>
        <item name="*com.android.chrome:colorAccent">@*android:color/accent_material_light</item>
        <item name="*com.android.chrome:colorControlNormal">#1e1e1e</item>
        <item name="android:elevation">0.0dip</item>
        <item name="android:titleTextColor">#1e1e1e</item>
        <item name="android:windowLightStatusBar">true</item>
        <item name="android:colorControlNormal">#1e1e1e</item>
    </style>

    <style name="MainThemeBase" parent="@*com.android.chrome:style/Theme.AppCompat.Light.NoActionBar">
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:textColorHighlight">@*com.android.chrome:color/text_highlight_color</item>
        <item name="android:textColorLink">#000000</item>
        <item name="android:windowContentTransitions">true</item>
        <item name="android:colorAccent">@*android:color/accent_material_light</item>
        <item name="android:windowEnterTransition">@*com.android.chrome:transition/fade</item>
        <item name="android:windowExitTransition">@null</item>
        <item name="android:windowSharedElementEnterTransition">@*com.android.chrome:transition/move_image</item>
        <item name="android:windowSharedElementExitTransition">@*com.android.chrome:transition/move_image</item>
        <item name="android:windowAllowReturnTransitionOverlap">true</item>
        <item name="android:windowAllowEnterTransitionOverlap">false</item>
        <item name="*com.android.chrome:buttonStyle">@*com.android.chrome:style/MainButtonStyle</item>
        <item name="android:statusBarColor">#ffffff</item>
        <item name="*com.android.chrome:colorControlActivated">#1e1e1e</item>
        <item name="*com.android.chrome:colorControlNormal">#1e1e1e</item>
        <item name="*com.android.chrome:colorPrimaryDark">#ffffff</item>
        <item name="android:windowLightStatusBar">true</item>
    </style>


