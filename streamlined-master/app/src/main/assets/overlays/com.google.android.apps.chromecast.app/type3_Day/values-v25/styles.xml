<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="Platform.V25.AppCompat.Light" parent="@android:style/Theme.Material.Light.NoActionBar">
        <item name="android:colorControlNormal">#1e1e1e</item>
        <item name="android:titleTextColor">#1e1e1e</item>
        <item name="android:windowLightStatusBar">true</item>
        <item name="android:statusBarColor">@android:color/background_light</item>
        <item name="android:colorBackgroundFloating">@android:color/background_light</item>
        <item name="android:navigationBarColor">@android:color/background_light</item>
        <item name="android:windowBackground">@android:color/background_light</item>
    </style>

    <style name="ManageAccountChip" parent="@*com.google.android.apps.chromecast.app:style/Widget.GoogleMaterial.Chip.Assistive">
        <item name="*com.google.android.apps.chromecast.app:cardBackgroundColor">@*android:color/background_material_light</item>
        <item name="android:textColor">#1e1e1e</item>
        <item name="*com.google.android.apps.chromecast.app:chipStrokeColor">@*android:color/accent_material_light</item>
        <item name="*com.google.android.apps.chromecast.app:chipBackgroundColor">@*android:color/background_material_light</item>
    </style>
</resources>
