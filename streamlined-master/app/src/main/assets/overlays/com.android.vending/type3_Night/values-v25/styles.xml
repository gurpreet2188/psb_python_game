<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="Platform.V25.AppCompat.Light" parent="@android:style/Theme.Material.NoActionBar">
        <item name="android:navigationBarColor">@android:color/background_dark</item>
        <item name="android:windowBackground">@android:color/background_dark</item>
    </style>

    <style name="LeftNavRedesignWhiteTheme" parent="@*com.android.vending:style/Theme.AppCompat">
        <item name="android:textColorPrimary">#ffffff</item>
        <item name="android:textAppearanceMedium">@*com.android.vending:style/DarkerMedium</item>
        <item name="android:windowBackground">@android:color/background_dark</item>
        <item name="android:textColorTertiary">#c1ffffff</item>
        <item name="android:windowLightStatusBar">false</item>
        <item name="*com.android.vending:colorPrimary">#ffffff</item>
        <item name="*com.android.vending:colorPrimaryDark">#ffffff</item>
        <item name="*com.android.vending:delegateToSupportLibAlertDialog">true</item>
    </style>

    <style name="PopupRedirectActivityThemeBottomSheet" parent="@*com.android.vending:style/PopupRedirectActivityTheme.BottomSheet">
        <item name="android:windowLightStatusBar">false</item>
        <item name="*com.android.vending:colorPrimary">@android:color/background_dark</item>
        <item name="*com.android.vending:colorPrimaryDark">@android:color/background_dark</item>
    </style>
</resources>
