<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="LatinMaterialDarkTheme">
        <item name="android:backgroundTint">@android:color/background_dark</item>
        <item name="*com.google.android.inputmethod.latin:Icon9KeyShiftUnderline">@*com.google.android.inputmethod.latin:drawable/ic_9key_shift_underline_material_dark_theme</item>
        <item name="*com.google.android.inputmethod.latin:Icon9KeyShiftUnderlineHighlight">@*com.google.android.inputmethod.latin:drawable/ic_9key_shift_underline_highlight_material_dark_theme</item>
        <item name="*com.google.android.inputmethod.latin:IconCiSimplified">@*com.google.android.inputmethod.latin:drawable/ic_key_ci_white_s</item>
        <item name="*com.google.android.inputmethod.latin:IconCiTraditional">@*com.google.android.inputmethod.latin:drawable/ic_key_ci_white_t</item>
        <item name="*com.google.android.inputmethod.latin:IconIndicMoreAlpha">204</item>
        <item name="*com.google.android.inputmethod.latin:IconMic">@*com.google.android.inputmethod.latin:drawable/ic_mic_dark_theme</item>
        <item name="*com.google.android.inputmethod.latin:colorLabelDynamic">@*com.google.android.inputmethod.latin:color/label_dynamic_light_green</item>
        <item name="*com.google.android.inputmethod.latin:iconTabKey">@*com.google.android.inputmethod.latin:drawable/sym_keyboard_tab_lxx_dark</item>
    </style>


    <style name="Body">
        <item name="android:textColor">#ffffff</item>
        <item name="android:tag">.keyboard-body-area</item>
        <item name="android:background">@android:color/background_dark</item>
        <item name="android:backgroundTint">@android:color/background_dark</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">?*com.google.android.inputmethod.latin:KeyboardBodyHeight</item>
    </style>

    <style name="CardViewerQueryText">
        <item name="android:textSize">@*com.google.android.inputmethod.latin:dimen/search_box_font_size</item>
        <item name="android:textColor">#d8ffffff</item>
        <item name="android:ellipsize">end</item>
        <item name="android:gravity">start|center</item>
        <item name="android:id">@*com.google.android.inputmethod.latin:id/card_viewer_query_text</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">fill_parent</item>
        <item name="android:maxLines">1</item>
        <item name="android:singleLine">true</item>
    </style>

    <style name="GBoardEditText">
        <item name="android:textSize">@*com.google.android.inputmethod.latin:dimen/search_box_font_size</item>
        <item name="android:textColor">#ffffff</item>
        <item name="android:ellipsize">end</item>
        <item name="android:background">@android:color/transparent</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">fill_parent</item>
        <item name="android:maxLines">1</item>
        <item name="android:imeOptions">actionSearch</item>
    </style>
    <style name="TranslateQueryEditor">
        <item name="android:textSize">@*com.google.android.inputmethod.latin:dimen/translate_board_editbox_font_size</item>
        <item name="android:textColor">#ffffff</item>
        <item name="android:ellipsize">start</item>
        <item name="android:background">@*com.google.android.inputmethod.latin:drawable/bg_translate_editbox_corners</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">fill_parent</item>
        <item name="android:hint">@*com.google.android.inputmethod.latin:string/translate_query_editbox_hint</item>
        <item name="android:maxLines">1</item>
        <item name="android:maxLength">200</item>
        <item name="android:inputType">text</item>
        <item name="android:imeOptions">actionDone</item>
        <item name="android:paddingStart">10.0dip</item>
    </style>
    <style name="AppThemeSelector" parent="@android:style/Theme.Material" />

    <style name="FirstRunViewPager">
        <item name="android:background">@android:color/background_dark</item>
        <item name="android:paddingBottom">@*com.google.android.inputmethod.latin:dimen/first_run_page_footer_height</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">fill_parent</item>
    </style>
    <style name="FirstRunPageDescription">
        <item name="android:textSize">@*com.google.android.inputmethod.latin:dimen/first_run_page_description_font_size</item>
        <item name="android:textColor">#c1ffffff</item>
        <item name="android:textColorLink">@*com.google.android.inputmethod.latin:color/first_run_page_blue_color</item>
        <item name="android:gravity">center_horizontal</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginTop">@*com.google.android.inputmethod.latin:dimen/first_run_page_description_padding_top</item>
        <item name="android:lineSpacingMultiplier">1.2</item>
    </style>
    <style name="FirstRunPageFooter">
        <item name="android:layout_gravity">bottom</item>
        <item name="android:orientation">vertical</item>
        <item name="android:background">@android:color/background_dark</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">@*com.google.android.inputmethod.latin:dimen/first_run_page_footer_height</item>
    </style>
    <style name="ThemeDetailsApplyButtonHolder">
        <item name="android:background">@android:color/background_dark</item>
        <item name="android:paddingLeft">60.0dip</item>
        <item name="android:paddingTop">24.0dip</item>
        <item name="android:paddingRight">60.0dip</item>
        <item name="android:paddingBottom">48.0dip</item>
        <item name="android:clipToPadding">false</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
    </style>
    <style name="FirstRunPageTitle">
        <item name="android:textSize">@*com.google.android.inputmethod.latin:dimen/first_run_page_title_font_size</item>
        <item name="android:textColor">#ffffff</item>
        <item name="android:gravity">center_horizontal</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginTop">@*com.google.android.inputmethod.latin:dimen/first_run_page_title_padding_top</item>
    </style>
    <style name="ThemeDetailsKeyBorderSwitch">
        <item name="android:textSize">16.0sp</item>
        <item name="android:textColor">#ffffffff</item>
        <item name="android:id">@*com.google.android.inputmethod.latin:id/theme_details_switch_key_border</item>
        <item name="android:paddingLeft">12.0dip</item>
        <item name="android:paddingRight">12.0dip</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:minHeight">48.0dip</item>
        <item name="android:text">@*com.google.android.inputmethod.latin:string/theme_details_button_title_key_border</item>
        <item name="android:switchPadding">12.0dip</item>
    </style>

    <style name="ThemeDetailsPreviewHolder">
        <item name="android:orientation">vertical</item>
        <item name="android:background">@android:color/holo_purple</item>
        <item name="android:paddingLeft">36.0dip</item>
        <item name="android:paddingTop">24.0dip</item>
        <item name="android:paddingRight">36.0dip</item>
        <item name="android:paddingBottom">24.0dip</item>
        <item name="android:clipToPadding">false</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
    </style>
    <style name="ThemeDetailsSwitchHolder">
        <item name="android:gravity">center_vertical</item>
        <item name="android:orientation">horizontal</item>
        <item name="android:background">@android:color/background_dark</item>
        <item name="android:paddingLeft">24.0dip</item>
        <item name="android:paddingTop">12.0dip</item>
        <item name="android:paddingRight">24.0dip</item>
        <item name="android:paddingBottom">12.0dip</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="AccessPointsPanel">
        <item name="android:tag">.access-points-panel</item>
        <item name="android:background">@android:color/background_dark</item>
        <item name="android:backgroundTint">@android:color/background_dark</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">fill_parent</item>
        <item name="*com.google.android.inputmethod.latin:itemHolderLayout">@*com.google.android.inputmethod.latin:layout/softkeyview_access_point_panel_item</item>
        <item name="*com.google.android.inputmethod.latin:itemLayout">@*com.google.android.inputmethod.latin:layout/softkey_access_point_panel_item</item>
    </style>

    <style name="TouchAware">
        <item name="android:tint">#ffffff</item>
        <item name="android:background">@android:color/transparent</item>
    </style>

    <style name="SuggestionStripAccessPointsMenuIcon">
        <item name="android:gravity">center</item>
        <item name="android:layout_gravity">start|center</item>
        <item name="android:tag">.keytop.for-prime-header-tab</item>
        <item name="android:background">@android:color/background_dark</item>
        <item name="android:backgroundTint">@android:color/background_dark</item>
        <item name="android:visibility">gone</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">fill_parent</item>
        <item name="android:scaleType">center</item>
        <item name="android:minWidth">@*com.google.android.inputmethod.latin:dimen/suggestions_strip_access_point_key_width</item>
    </style>
