<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="common.Theme.Base.GoogleSettings" parent="@*com.google.android.gms:style/Theme.AppCompat.Light">
        <item name="android:navigationBarColor">@android:color/background_light</item>
        <item name="android:colorControlNormal">#1e1e1e</item>
        <item name="android:windowLightNavigationBar">true</item>
        <item name="android:windowLightStatusBar">true</item>
        <item name="android:windowBackground">@android:color/background_light</item>
        <item name="android:titleTextColor">#1e1e1e</item>
        <item name="android:listPreferredItemHeight">72.0dip</item>
        <item name="android:statusBarColor">@android:color/holo_red_light</item>
        <item name="android:colorEdgeEffect">@*android:color/accent_material_light</item>
        <item name="*com.google.android.gms:popupTheme">@*com.google.android.gms:style/ThemeOverlay.AppCompat.Light</item>
        <item name="*com.google.android.gms:actionBarTheme">@*com.google.android.gms:style/common.Theme.Base.ActionBar</item>
        <item name="*com.google.android.gms:colorPrimary">@android:color/holo_red_light</item>
        <item name="*com.google.android.gms:colorPrimaryDark">@android:color/holo_red_light</item>
        <item name="android:colorAccent">@*android:color/accent_material_light</item>
        <item name="*com.google.android.gms:colorAccent">#1e1e1e</item>
        <item name="*com.google.android.gms:preferenceTheme">@*com.google.android.gms:style/PreferenceTheme</item>
    </style>
</resources>