<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="MainTheme" parent="@*com.android.chrome:style/MainThemeBase">
        <item name="android:navigationBarColor">@android:color/background_light</item>
        <item name="android:windowLightNavigationBar">true</item>
    </style>
    <style name="FullscreenWhite" parent="@*com.android.chrome:style/FullscreenWhiteBase">
        <item name="android:navigationBarColor">@android:color/background_light</item>
        <item name="android:windowLightNavigationBar">true</item>
    </style>
    <style name="ThemeWithActionBar" parent="@*com.android.chrome:style/ThemeWithActionBarBase">
        <item name="android:navigationBarColor">@android:color/background_light</item>
        <item name="android:windowLightNavigationBar">true</item>
    </style>
    <style name="DialogWhenLarge" parent="@*com.android.chrome:style/DialogWhenLargeBase">
        <item name="android:navigationBarColor">@android:color/background_light</item>
        <item name="android:windowLightNavigationBar">true</item>
    </style>
    
</resources>