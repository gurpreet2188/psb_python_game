<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="DialtactsTheme" parent="@*com.google.android.dialer:style/Dialer.ThemeBase.ActionBar">
        <item name="android:navigationBarColor">?android:windowBackground</item>
        <item name="android:windowLightNavigationBar">true</item>
        <item name="android:navigationBarDividerColor">#00000000</item>
    </style>
    <style name="MainActivityTheme" parent="@*com.google.android.dialer:style/MainActivityThemeBase">
        <item name="android:navigationBarColor">?android:colorBackgroundFloating</item>
        <item name="android:windowLightNavigationBar">true</item>
        <item name="android:navigationBarDividerColor">#00000000</item>
    </style>
    <style name="MainActivityTheme.Dark" parent="@*com.google.android.dialer:style/MainActivityThemeBase.Dark">
        <item name="android:navigationBarColor">?android:colorBackgroundFloating</item>
        <item name="android:windowLightNavigationBar">true</item>
        <item name="android:navigationBarDividerColor">#00000000</item>
    </style>

    <style name="SettingsStyle" parent="@*com.google.android.dialer:style/Dialer.ThemeBase.NoActionBar">
        <item name="android:navigationBarColor">@android:color/background_light</item>
        <item name="android:colorAccent">@*android:color/accent_material_light</item>
        <item name="android:windowBackground">@android:color/background_light</item>
        <item name="android:colorPrimary">@android:color/holo_red_light</item>
        <item name="android:colorPrimaryDark">@android:color/holo_red_light</item>
        <item name="android:colorSecondary">@android:color/holo_red_light</item>
        <item name="android:navigationBarDividerColor">#00000000</item>
    </style>
</resources>
