<?xml version="1.0" encoding="utf-8"?>
<resources>



    <style name="BugleBaseTheme.ActionBar" parent="@*com.google.android.apps.messaging:style/BugleBaseThemeDark.ActionBar" />
    <style name="BugleBaseTheme.NoActionBar" parent="@*com.google.android.apps.messaging:style/BugleBaseThemeDark.NoActionBar"/>

    <style name="BugleBaseThemeDark.ActionBar" parent="@*com.google.android.apps.messaging:style/BugleBaseThemeDark">
        <item name="*com.google.android.apps.messaging:actionBarBackgroundColorM2">@android:color/background_dark</item>
        <item name="android:windowDrawsSystemBarBackgrounds">true</item>
        <item name="android:statusBarColor">@android:color/holo_blue_light</item>
        <item name="android:navigationBarColor">@android:color/background_dark</item>
        <item name="android:windowLightNavigationBar">false</item>
        <item name="android:navigationBarDividerColor">#00000000</item>
    </style>
    <style name="BugleBaseThemeDark.NoActionBar" parent="@*com.google.android.apps.messaging:style/BugleBaseThemeDark">
        <item name="*com.google.android.apps.messaging:actionBarBackgroundColorM2">@android:color/background_dark</item>
        <item name="android:windowDrawsSystemBarBackgrounds">true</item>
        <item name="android:statusBarColor">@android:color/holo_blue_light</item>
        <item name="android:navigationBarColor">@android:color/background_dark</item>
        <item name="android:windowLightNavigationBar">false</item>
        <item name="android:navigationBarDividerColor">#00000000</item>
        <item name="*com.google.android.apps.messaging:windowActionBar">false</item>
        <item name="*com.google.android.apps.messaging:windowNoTitle">true</item>
    </style>

    <style name="BuglePhotoViewTheme" parent="@*com.google.android.apps.messaging:style/PhotoViewTheme.Translucent">
        <item name="android:statusBarColor">#000000</item>
        <item name="android:navigationBarColor">@android:color/background_dark</item>
        <item name="android:windowLightNavigationBar">false</item>
        <item name="android:navigationBarDividerColor">#00000000</item>
        <item name="actionModeShareDrawable">@*com.google.android.apps.messaging:drawable/ic_share_light</item>
    </style>

    <style name="StickerM2LightTheme" parent="@*com.google.android.apps.messaging:style/Theme.GoogleMaterial.Light.NoActionBar">
        <item name="android:windowDrawsSystemBarBackgrounds">true</item>
        <item name="android:statusBarColor">@android:color/holo_blue_light</item>
        <item name="android:navigationBarColor">@android:color/background_dark</item>
        <item name="android:windowLightNavigationBar">false</item>
        <item name="android:navigationBarDividerColor">#00000000</item>
    </style>
    <style name="Theme.GoogleMaterial.Light" parent="@*com.google.android.apps.messaging:style/Base.Theme.GoogleMaterial.Light">
        <item name="android:navigationBarColor">@android:color/background_dark</item>
        <item name="android:windowLightNavigationBar">false</item>
        <item name="android:navigationBarDividerColor">#00000000</item>
    </style>
    <style name="Theme.GoogleMaterial.Light.BottomSheetDialog" parent="@*com.google.android.apps.messaging:style/Base.Theme.GoogleMaterial.Light.BottomSheetDialog">
        <item name="android:navigationBarColor">@android:color/background_dark</item>
        <item name="android:windowLightNavigationBar">false</item>
        <item name="android:navigationBarDividerColor">#00000000</item>
    </style>
    <style name="BugleDarkNavBarTheme" parent="@*com.google.android.apps.messaging:style/BugleToolbarTheme">
        <item name="android:navigationBarColor">@android:color/background_dark</item>
        <item name="android:windowLightNavigationBar">false</item>
        <item name="android:navigationBarDividerColor">#00000000</item>
    </style>
</resources>