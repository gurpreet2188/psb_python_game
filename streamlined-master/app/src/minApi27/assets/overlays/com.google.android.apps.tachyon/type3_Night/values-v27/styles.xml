<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="TachyonBaseTheme" parent="@*com.google.android.apps.tachyon:style/Theme.GoogleMaterial.Light.NoActionBar">
        <item name="android:popupMenuStyle">@*com.google.android.apps.tachyon:style/Widget.GoogleMaterial.PopupMenu.Overflow</item>
        <item name="android:windowLayoutInDisplayCutoutMode">shortEdges</item>
        <item name="android:windowLightNavigationBar">false</item>
        <item name="android:navigationBarColor">#66ffffff</item>
       
    </style>
    <style name="Theme.GoogleMaterial.Light" parent="@*com.google.android.apps.tachyon:style/Base.Theme.GoogleMaterial.Light">
        <item name="android:windowLightNavigationBar">false</item>
        <item name="android:navigationBarColor">@android:color/background_dark</item>
       
    </style>
    <style name="Theme.GoogleMaterial.Light.BottomSheetDialog" parent="@*com.google.android.apps.tachyon:style/Base.Theme.GoogleMaterial.Light.BottomSheetDialog">
        <item name="android:windowLightNavigationBar">false</item>
        <item name="android:navigationBarColor">@android:color/background_dark</item>
    </style>
</resources>
