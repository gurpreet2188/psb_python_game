<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="Theme.GoogleMaterial.Light" parent="@*com.google.android.play.games:style/Base.Theme.GoogleMaterial.Light">
        <item name="android:navigationBarColor">@android:color/background_dark</item>
        <item name="android:windowLightNavigationBar">false</item>
    </style>
    <style name="Theme.GoogleMaterial.Light.BottomSheetDialog" parent="@*com.google.android.play.games:style/Base.Theme.GoogleMaterial.Light.BottomSheetDialog">
