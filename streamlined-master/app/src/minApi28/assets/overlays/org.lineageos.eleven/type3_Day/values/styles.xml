<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="Eleven.Theme" parent="@android:style/Theme.Material.Light">
        <item name="android:colorControlNormal">#1e1e1e</item>
        <item name="android:titleTextColor">#1e1e1e</item>
        <item name="android:windowLightStatusBar">true</item>
        <item name="android:windowLightNavigationBar">true</item>
        <item name="android:navigationBarColor">@android:color/background_light</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:dropDownListViewStyle">@*org.lineageos.eleven:style/DropDownListView</item>
        <item name="android:listViewStyle">@*org.lineageos.eleven:style/ListView</item>
        <item name="android:actionBarStyle">@*org.lineageos.eleven:style/ActionBar</item>
        <item name="android:popupMenuStyle">@*org.lineageos.eleven:style/PopupMenu</item>
        <item name="android:textAppearanceLargePopupMenu">@*org.lineageos.eleven:style/LargePopupMenu</item>
        <item name="android:textAppearanceSmallPopupMenu">@*org.lineageos.eleven:style/SmallPopupMenu</item>
        <item name="android:actionBarWidgetTheme">@*org.lineageos.eleven:style/ActionBarWidgetTheme</item>
        <item name="android:windowContentTransitions">true</item>
        <item name="android:colorPrimary">@android:color/holo_red_light</item>
        <item name="android:colorPrimaryDark">@android:color/holo_red_light</item>
        <item name="android:colorAccent">@*android:color/accent_material_light</item>
        <item name="android:windowAllowReturnTransitionOverlap">false</item>
        <item name="android:windowAllowEnterTransitionOverlap">false</item>
    </style>
</resources>