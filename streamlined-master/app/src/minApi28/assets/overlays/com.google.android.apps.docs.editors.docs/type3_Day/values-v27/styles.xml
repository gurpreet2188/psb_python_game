<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="CakemixTheme.DocListActivity.OverrideGm2.Dialog" parent="@*com.google.android.apps.docs.editors.docs:style/Theme.AppCompat.DayNight.DialogWhenLarge">
        <item name="android:windowLightStatusBar">true</item>
        <item name="android:windowLightNavigationBar">true</item>
        <item name="*com.google.android.apps.docs.editors.docs:colorAccent">@android:color/holo_blue_bright</item>
    </style>
    <style name="Theme.GoogleMaterial.Light" parent="@*com.google.android.apps.docs.editors.docs:style/Base.Theme.GoogleMaterial.Light">
        <item name="android:navigationBarColor">@android:color/background_light</item>
        <item name="android:windowLightNavigationBar">true</item>
    </style>
    <style name="Theme.GoogleMaterial.Light.BottomSheetDialog" parent="@*com.google.android.apps.docs.editors.docs:style/Base.Theme.GoogleMaterial.Light.BottomSheetDialog">
        <item name="android:navigationBarColor">@android:color/background_light</item>
        <item name="android:windowLightNavigationBar">true</item>
        <item name="android:navigationBarDividerColor">@android:color/transparent</item>
    </style>
</resources>
