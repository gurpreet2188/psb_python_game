<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="ContactsTheme" parent="@*com.google.android.contacts:style/BaseContactsTheme">
        <item name="android:colorBackgroundFloating">@android:color/background_dark</item>
        <item name="android:navigationBarColor">@android:color/background_dark</item>
        <item name="android:windowLightNavigationBar">false</item>
    </style>

    <style name="Theme.GoogleMaterial.Light" parent="@*com.google.android.contacts:style/Theme.GoogleMaterial.Dark">
        <item name="android:colorBackgroundFloating">@android:color/background_dark</item>
        <item name="android:navigationBarColor">@android:color/background_dark</item>
        <item name="android:windowLightNavigationBar">false</item>
    </style>
    <style name="Theme.GoogleMaterial.Light.BottomSheetDialog" parent="@*com.google.android.contacts:style/Theme.GoogleMaterial.Dark.BottomSheetDialog">
        <item name="android:navigationBarColor">@android:color/background_dark</item>
        <item name="android:windowLightNavigationBar">false</item>
    </style>
</resources>