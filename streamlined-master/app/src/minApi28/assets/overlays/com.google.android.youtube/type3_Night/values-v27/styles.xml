<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="Theme.YouTube.Light" parent="@*com.google.android.youtube:style/Base.V27.Theme.YouTube.Dark">
        <item name="android:textColorPrimary">#ffffff</item>
    </style>

    <style name="Base.V27.Theme.YouTube.Dark" parent="@*com.google.android.youtube:style/Base.Theme.YouTube.Dark">
        <item name="android:textColorSecondaryInverse">#d8ffffff</item>
        <item name="android:textColorPrimaryInverse">#ffffff</item>
        <item name="android:textColorPrimary">#ffffff</item>
        <item name="android:textColorSecondary">#ffffff</item>
        <item name="android:navigationBarColor">@android:color/background_dark</item>
        <item name="android:windowLightNavigationBar">false</item>
    </style>
    <style name="Theme.YouTube.Home" parent="@*com.google.android.youtube:style/Base.V27.Theme.YouTube.Home">
        <item name="android:textColorSecondaryInverse">#d8ffffff</item>
        <item name="android:textColorPrimaryInverse">#ffffff</item>
        <item name="android:textColorPrimary">#ffffff</item>
        <item name="android:navigationBarColor">@android:color/background_dark</item>
        <item name="android:windowLightNavigationBar">false</item>
    </style>

    <style name="Base.V27.Theme.YouTube.Home" parent="@*com.google.android.youtube:style/Base.Theme.YouTube.Home">
        <item name="android:textColorPrimary">#ffffff</item>
    </style>

    <style name="Base.V27.Theme.YouTube.Light" parent="@*com.google.android.youtube:style/Base.Theme.YouTube.Dark">
        <item name="android:textColorSecondaryInverse">#d8ffffff</item>
        <item name="android:textColorPrimaryInverse">#ffffff</item>
        <item name="android:textColorPrimary">#ffffff</item>
        <item name="android:textColorSecondary">#ffffff</item>
        <item name="android:navigationBarColor">@android:color/background_dark</item>
        <item name="android:windowLightNavigationBar">false</item>
    </style>

    <style name="Base.V27.Theme.YouTube.Light.Dialog" parent="@*com.google.android.youtube:style/Base.V23.Theme.YouTube.Light.Dialog">
         <item name="android:navigationBarColor">@android:color/background_dark</item>
        <item name="android:windowLightNavigationBar">false</item>
    </style>

    <style name="Base.V27.Theme.YouTube.Share.DialogFragment.Dark" parent="@*com.google.android.youtube:style/Base.Theme.YouTube.Share.DialogFragment.Dark">
        <item name="android:navigationBarColor">@android:color/background_dark</item>
        <item name="android:windowLightNavigationBar">false</item>
    </style>
    <style name="Base.V27.Theme.YouTube.Share.DialogFragment.Light" parent="@*com.google.android.youtube:style/Base.Theme.YouTube.Share.DialogFragment.Dark">
        <item name="android:navigationBarColor">@android:color/background_dark</item>
        <item name="android:windowLightNavigationBar">false</item>
    </style>

    <style name="NavigationBar.Dark.V27" parent="@*com.google.android.youtube:style/NavigationBar">
        <item name="*com.google.android.youtube:navigationBarColor">@android:color/background_dark</item>
        <item name="*com.google.android.youtube:navigationBarControlsMode">light</item>
        <item name="*com.google.android.youtube:navigationBarDividerColor">@null</item>
        <item name="*com.google.android.youtube:navigationBarDividerSize">0.0dip</item>
    </style>
    <style name="NavigationBar.Light.V27" parent="@*com.google.android.youtube:style/NavigationBar">
        <item name="*com.google.android.youtube:navigationBarColor">@android:color/background_dark</item>
        <item name="*com.google.android.youtube:navigationBarControlsMode">light</item>
        <item name="*com.google.android.youtube:navigationBarDividerColor">@null</item>
        <item name="*com.google.android.youtube:navigationBarDividerSize">0.0dip</item>
    </style>
</resources>
