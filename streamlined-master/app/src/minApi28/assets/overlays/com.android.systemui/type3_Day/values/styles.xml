    <style name="qs_theme" parent="@*android:style/Theme.Material.Light">
        <item name="*com.android.systemui:darkIconTheme">@*com.android.systemui:style/QSIconTheme</item>
        <item name="*com.android.systemui:lightIconTheme">@*com.android.systemui:style/QSIconTheme</item>
        <item name="android:colorPrimary">#1e1e1e</item>
        <item name="android:colorAccent">@*android:color/accent_material_light</item>
        <item name="android:colorSecondary">@*android:color/background_material_light</item>
    </style>

    <style name="qs_base" parent="@*android:style/Theme.Material.Light" >
        <item name="android:colorPrimary">@*android:color/background_material_light</item>
        <item name="android:colorPrimaryDark">@*android:color/background_material_light</item>
    </style>



    <style name="edit_theme" parent="@*android:style/Theme.Material.Light">
        <item name="android:colorBackground">@*android:color/background_material_light</item>
    </style>

    <style name="Theme.SystemUI.Light" parent="@*android:style/Theme.Material.Light">
        <item name="android:colorControlHighlight">#40000000</item>
        <item name="android:colorError">@*android:color/error_color_material_light</item>
        <item name="*com.android.systemui:passwordStyle">@*com.android.systemui:style/PasswordTheme.Light</item>
        <item name="*com.android.systemui:wallpaperTextColor">@*android:color/primary_text_material_light</item>
        <item name="*com.android.systemui:wallpaperTextColorSecondary">@*android:color/secondary_text_material_light</item>
    </style>

    <style name="Theme.SystemUI.Dialog" parent="@android:style/Theme.DeviceDefault.Light.Dialog">
        <item name="android:colorAccent">@*android:color/accent_material_light</item>
        <item name="android:colorControlActivated">@*android:color/accent_material_light</item>
        <item name="android:colorPrimary">@*android:color/accent_material_light</item>
    </style>
    <style name="Theme.SystemUI.Dialog.Alert" parent="@android:style/Theme.DeviceDefault.Light.Dialog.Alert">
        <item name="android:colorAccent">@*android:color/accent_material_light</item>
    </style>
    <style name="Theme.AlertDialogHost" parent="@android:style/Theme.DeviceDefault.Light">
        <item name="android:windowBackground">@android:color/background_light</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:backgroundDimEnabled">false</item>
    </style>
    <style name="TextAppearance.NotificationGuts" parent="">
        <item name="android:textSize">14.0sp</item>
        <item name="android:textColor">#1e1e1e</item>
        <item name="android:fontFamily">roboto-regular</item>
    </style>
    <style name="TextAppearance.NotificationGuts.Secondary" parent="">
        <item name="android:textColor">#6e6e6e</item>
        <item name="android:fontFamily">roboto-regular</item>
        <item name="android:textSize">14.0sp</item>
    </style>
    <style name="TextAppearance.NotificationGuts.Primary" parent="">
        <item name="android:fontFamily">roboto-regular</item>
        <item name="android:textSize">16.0sp</item>
        <item name="android:textColor">#1e1e1e</item>
    </style>
    <style name="TextAppearance.NotificationGuts.Radio" parent="">
        <item name="android:textColor">#1e1e1e</item>
        <item name="android:fontFamily">roboto-regular</item>
        <item name="android:textSize">14.0sp</item>
    </style>

    <style name="TextAppearance.QS.DetailItemSecondary" parent="">
        <item name="android:textSize">14.0sp</item>
        <item name="android:textColor">#6e6e6e</item>
        <item name="android:fontFamily">sans-serif</item>
    </style>

    <style name="QSBorderlessButton">
        <item name="android:tint">#1e1e1e</item>
        <item name="android:gravity">center</item>
        <item name="android:background">#00ffffff</item>
        <item name="android:padding">12.0dip</item>
    </style>

    <style name="TextAppearance.QS.DetailButton" parent="">
        <item name="android:textStyle">normal</item>
        <item name="android:textSize">14.0sp</item>
        <item name="android:textColor">#1e1e1e</item>
        <item name="android:gravity">center</item>
        <item name="android:textAllCaps">true</item>
        <item name="android:fontFamily">sans-serif-medium</item>
    </style>

    <style name="TextAppearance.QS.DetailButton.White" parent="">
        <item name="android:textStyle">normal</item>
        <item name="android:textSize">14.0sp</item>
        <item name="android:textColor">#1e1e1e</item>
        <item name="android:gravity">center</item>
        <item name="android:textAllCaps">true</item>
        <item name="android:fontFamily">sans-serif-medium</item>
    </style>

    <style name="TextAppearance.QS" parent="">
        <item name="android:textStyle">normal</item>
        <item name="android:textColor">#1e1e1e</item>
        <item name="android:fontFamily">sans-serif</item>
    </style>

    <style name="TextAppearance.StatusBar.Clock" parent="@*android:style/TextAppearance.StatusBar.Icon">
        <item name="android:textSize">14sp</item>
        <item name="android:fontFamily">sans-serif-medium</item>
        <item name="android:textColor">#1e1e1e</item>
    </style>

    <style name="TextAppearance.StatusBar.Expanded" parent="@*android:style/TextAppearance.StatusBar" />

    <style name="TextAppearance.StatusBar.Expanded.Clock">
        <item name="android:textSize">20sp</item>
        <item name="android:fontFamily">sans-serif-medium</item>
        <item name="android:textColor">#1e1e1e</item>
    </style>

    <style name="TextAppearance.StatusBar.Expanded.Date">
        <item name="android:textSize">14sp</item>
        <item name="android:textStyle">normal</item>
        <item name="android:textColor">#b21e1e1e</item>
    </style>

    <style name="TextAppearance.StatusBar.Expanded.AboveDateTime">
        <item name="android:textSize">12sp</item>
        <item name="android:textStyle">normal</item>
        <item name="android:textColor">#a11e1e1e</item>
    </style>

    <style name="TextAppearance.StatusBar.Expanded.Weather.Detailed" parent="">
        <item name="android:textSize">14sp</item>
        <item name="android:textStyle">normal</item>
        <item name="android:textColor">#1e1e1e</item>
    </style>

    <style name="TextAppearance.StatusBar.WeatherTemp" parent="@*android:style/TextAppearance.StatusBar">
        <item name="android:textStyle">normal</item>
    </style>

    <style name="TextAppearance.StatusBar.Expanded.WeatherTemp">
        <item name="android:textSize">10sp</item>
        <item name="android:textStyle">normal</item>
        <item name="android:textColor">#1e1e1e</item>
    </style>

    <style name="TextAppearance.StatusBar.Expanded.WeatherLocation">
        <item name="android:textSize">10sp</item>
        <item name="android:textStyle">normal</item>
        <item name="android:textColor">#d81e1e1e</item>
    </style>

    <style name="DualToneDarkTheme">
        <item name="*com.android.systemui:backgroundColor">@*com.android.systemui:color/dark_mode_icon_color_dual_tone_background</item>
        <item name="*com.android.systemui:fillColor">@*com.android.systemui:color/dark_mode_icon_color_dual_tone_fill</item>
        <item name="*com.android.systemui:singleToneColor">@*com.android.systemui:color/dark_mode_icon_color_single_tone</item>
        <item name="android:colorControlNormal">#1e1e1e</item>
    </style>
    <style name="DualToneLightTheme">
        <item name="*com.android.systemui:backgroundColor">@*com.android.systemui:color/light_mode_icon_color_dual_tone_background</item>
        <item name="*com.android.systemui:fillColor">@*com.android.systemui:color/light_mode_icon_color_dual_tone_fill</item>
        <item name="*com.android.systemui:singleToneColor">@*com.android.systemui:color/light_mode_icon_color_single_tone</item>
        <item name="android:colorControlNormal">#f1f1f1</item>
    </style>


</resources>