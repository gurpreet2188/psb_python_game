    <style name="DualToneDarkTheme">
        <item name="*com.android.systemui:backgroundColor">@*com.android.systemui:color/dark_mode_icon_color_dual_tone_background</item>
        <item name="*com.android.systemui:fillColor">@*com.android.systemui:color/dark_mode_icon_color_dual_tone_fill</item>
        <item name="*com.android.systemui:singleToneColor">@*com.android.systemui:color/dark_mode_icon_color_single_tone</item>
        <item name="android:colorControlNormal">#1e1e1e</item>
    </style>
    <style name="DualToneLightTheme">
        <item name="*com.android.systemui:backgroundColor">@*com.android.systemui:color/light_mode_icon_color_dual_tone_background</item>
        <item name="*com.android.systemui:fillColor">@*com.android.systemui:color/light_mode_icon_color_dual_tone_fill</item>
        <item name="*com.android.systemui:singleToneColor">@*com.android.systemui:color/light_mode_icon_color_single_tone</item>
        <item name="android:colorControlNormal">#f1f1f1</item>
    </style>

    <style name="Theme.SystemUI.Dialog.GlobalActions" parent="@*android:style/Theme.DeviceDefault.NoActionBar.Fullscreen">

        <item name="android:windowIsFloating">true</item>
    </style>

    <style name="qs_base" parent="@*android:style/Theme.Material" >
        <item name="android:colorPrimary">@android:color/holo_purple</item>
        <item name="android:colorPrimaryDark">@android:color/holo_purple</item>
    </style>

    <style name="edit_theme" parent="@*android:style/Theme.Material">
        <item name="android:colorBackground">@android:color/holo_purple</item>
        <item name="android:windowBackground">@android:color/holo_purple</item>
    </style>

    <style name="hybrid_notification_text">
        <item name="android:textColor">@*android:color/notification_default_color_light</item>
        <item name="android:paddingEnd">4.0dip</item>
    </style>

